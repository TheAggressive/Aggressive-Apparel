{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aggressive-apparel/navigation",
  "version": "1.0.0",
  "title": "Navigation",
  "category": "aggressive-apparel",
  "description": "Responsive navigation container with mobile panel support.",
  "keywords": ["menu", "navigation", "header", "nav"],
  "textdomain": "aggressive-apparel",
  "icon": "menu",
  "attributes": {
    "breakpoint": {
      "type": "number",
      "default": 1024
    },
    "ariaLabel": {
      "type": "string",
      "default": "Main navigation"
    },
    "navId": {
      "type": "string",
      "default": ""
    },
    "openOn": {
      "type": "string",
      "enum": ["hover", "click"],
      "default": "hover"
    }
  },
  "supports": {
    "html": false,
    "interactivity": true,
    "align": ["wide", "full"],
    "color": {
      "background": true,
      "text": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true
    },
    "__experimentalBorder": {
      "radius": true,
      "width": true,
      "color": true,
      "style": true
    },
    "shadow": true,
    "layout": {
      "default": {
        "type": "flex",
        "flexWrap": "nowrap",
        "justifyContent": "space-between",
        "orientation": "horizontal"
      },
      "allowSwitching": true,
      "allowInheriting": false,
      "allowEditing": true,
      "allowJustification": true,
      "allowOrientation": false,
      "allowVerticalAlignment": true
    },
    "className": true
  },
  "providesContext": {
    "aggressive-apparel/navigationBreakpoint": "breakpoint",
    "aggressive-apparel/navigationOpenOn": "openOn",
    "aggressive-apparel/navigationId": "navId"
  },
  "viewScriptModule": "file:./view.js",
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
