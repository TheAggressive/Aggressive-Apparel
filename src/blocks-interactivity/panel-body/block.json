{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aggressive-apparel/panel-body",
  "version": "1.0.0",
  "title": "Panel Body",
  "category": "aggressive-apparel",
  "description": "Main content area for the navigation panel. Contains navigation menus and other content.",
  "keywords": ["body", "content", "panel", "navigation"],
  "textdomain": "aggressive-apparel",
  "icon": "align-center",
  "parent": ["aggressive-apparel/navigation-panel"],
  "attributes": {
    "isPanel": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": false
    },
    "className": true
  },
  "usesContext": ["aggressive-apparel/navigationId"],
  "providesContext": {
    "aggressive-apparel/isInsidePanel": "isPanel"
  },
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
