{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aggressive-apparel/panel-close-button",
  "version": "1.0.0",
  "title": "Panel Close Button",
  "category": "aggressive-apparel",
  "description": "Close button for the navigation panel.",
  "keywords": ["close", "button", "panel", "navigation"],
  "textdomain": "aggressive-apparel",
  "icon": "dismiss",
  "parent": ["aggressive-apparel/navigation-panel", "core/group"],
  "ancestor": ["aggressive-apparel/navigation-panel"],
  "attributes": {
    "iconColor": {
      "type": "string"
    },
    "iconHoverColor": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundHoverColor": {
      "type": "string"
    },
    "label": {
      "type": "string",
      "default": "Close menu"
    }
  },
  "supports": {
    "html": false,
    "interactivity": true,
    "spacing": {
      "margin": true
    },
    "__experimentalBorder": {
      "radius": true
    },
    "className": true
  },
  "usesContext": ["aggressive-apparel/navigationId"],
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
