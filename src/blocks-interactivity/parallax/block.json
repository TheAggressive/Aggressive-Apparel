{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aggressive-apparel/parallax",
  "version": "1.0.8",
  "title": "Advanced Parallax Container",
  "category": "widgets",
  "icon": "cover-image",
  "description": "A container block that applies parallax effects to nested blocks using intersection observer.",
  "supports": {
    "html": false,
    "interactivity": true,
    "layout": {
      "allowOrientation": false,
      "allowEditing": false
    },
    "align": ["wide", "full"]
  },
  "attributes": {
    "intensity": {
      "type": "number",
      "default": 50
    },
    "visibilityTrigger": {
      "type": "number",
      "default": 0.3
    },
    "detectionBoundary": {
      "type": "object",
      "default": {
        "top": "0%",
        "right": "0%",
        "bottom": "0%",
        "left": "0%"
      }
    },
    "enableMouseInteraction": {
      "type": "boolean",
      "default": false
    },
    "debugMode": {
      "type": "boolean",
      "default": false
    },
    "parallaxDirection": {
      "type": "string",
      "default": "down"
    },
    "mouseInfluenceMultiplier": {
      "type": "number",
      "default": 0.5
    },
    "maxMouseTranslation": {
      "type": "number",
      "default": 20
    },
    "mouseSensitivityThreshold": {
      "type": "number",
      "default": 0.001
    },
    "depthIntensityMultiplier": {
      "type": "number",
      "default": 50
    },
    "transitionDuration": {
      "type": "number",
      "default": 0.1
    },
    "perspectiveDistance": {
      "type": "number",
      "default": 1000
    },
    "maxMouseRotation": {
      "type": "number",
      "default": 5
    },
    "parallaxDepth": {
      "type": "number",
      "default": 1.0
    }
  },
  "providesContext": {
    "aggressive-apparel/parallax/intensity": "intensity",
    "aggressive-apparel/parallax/isIntersecting": "isIntersecting",
    "aggressive-apparel/parallax/scrollProgress": "scrollProgress"
  },
  "textdomain": "aggressive-apparel",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}
