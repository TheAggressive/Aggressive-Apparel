        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      }
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontSize": "var:preset|font-size|xxx-large",
          "fontWeight": "700",
          "textTransform": "uppercase"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|primary"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|accent"
              }
            }
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var:preset|font-size|medium",
          "fontWeight": "500",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        },
        "spacing": {
          "blockGap": "var:preset|spacing|7"
        }
      },
      "core/post-title": {
        "typography": {
          "fontSize": "var:preset|font-size|xxxx-large",
          "fontWeight": "700"
        }
      },
      "core/post-content": {
        "spacing": {
          "blockGap": "var:preset|spacing|7"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "var:preset|spacing|12"
        }
      },
      "core/columns": {
        "spacing": {
          "blockGap": "var:preset|spacing|2"
        }
      },
      "core/separator": {
        "color": {
