  "styles": {
    "css": ":root { color-scheme: light dark; } ::selection { background-color: var(--wp--preset--color--red); color: var(--wp--preset--color--white); }",
    "color": {
      "background": "var:preset|color|light-dark-white-black",
      "text": "var:preset|color|light-dark-black-white"
    },
    "typography": {
      "fontFamily": "var:preset|font-family|system-sans-serif",
      "fontSize": "var:preset|font-size|medium",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "var:preset|spacing|8"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var:preset|color|primary"
        },
        ":hover": {
          "color": {
            "text": "var:preset|color|accent"
          }
        },
        ":focus": {
          "color": {
            "text": "var:preset|color|accent"
          }
        }
      },
      "button": {
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|white"
        },
        "typography": {
          "fontSize": "var:preset|font-size|medium",
          "fontWeight": "600",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        },
        "border": {
          "radius": "calc(infinity * 1px)"
        },
        ":hover": {
          "color": {
            "background": "var:preset|color|white",
            "text": "var:preset|color|red"
          }
        },
        ":focus": {
          "color": {
            "background": "var:preset|color|accent",
            "text": "var:preset|color|base"
          }
        },
        ":active": {
          "color": {
            "background": "var:preset|color|accent",
            "text": "var:preset|color|base"
          }
        }
      },
      "heading": {
        "color": {
          "text": "var:preset|color|primary"
        },
        "typography": {
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xxxxx-large"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|xxxx-large"
        }
      },
      "h3": {
